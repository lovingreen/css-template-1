<header class="header">
    {{#with header}}
        <div class="header__top">
            <a href="/" class="button">{{brandName}}</a>
            <ul class="header__menu">
                {{#each links}}
                <li class="menu__item"><a href="{{url}}">{{text}}</a></li>  
                {{/each}}
            </ul>
        </div>
        <div class="header__ad">
            <div class="header__container">
                <div class="header__addbadge">Ad</div>
                <a class="header__text" href="{{ad.url}}">{{ad.text}}</a>
            </div>
            <div class="header_users">Users online: {{userOnline}}</div>
        </div>
    {{/with}}
</header>